{
  "name": "pbr5",
  "version": "0.0.1",
  "description": "Sketching library for Physically Based Rendering",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others -p \"[{name}]\" -n \"pbr5,blog\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run pbr5\" \"npm run blog\"",
    "pbr5": "webpack-dev-server --open",
    "blog": "cd docs && bundle exec jekyll serve --drafts --livereload & sleep 2 && open http://127.0.0.1:4000/pbr5/",
    "api_doc": "typedoc --theme minimal --out ./api_doc/ src/app.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbakse/pbr5.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/jbakse/pbr5/issues"
  },
  "homepage": "https://github.com/jbakse/pbr5#readme",
  "devDependencies": {
    "concurrently": "^3.5.0",
    "gl-matrix": "^2.4.0",
    "glslify-loader": "^1.0.2",
    "raw-loader": "^0.5.1",
    "script-loader": "^0.7.0",
    "source-map-loader": "^0.2.1",
    "ts-loader": "^2.3.4",
    "typescript": "^2.5.2",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "@types/file-saver": "0.0.1",
    "@types/gl-matrix": "^2.2.34",
    "@types/lodash": "^4.14.66",
    "file-saver": "^1.3.3",
    "gl-constants": "^1.0.0",
    "lodash": "^4.17.4",
    "polyline-normals": "^2.0.2"
  }
}
